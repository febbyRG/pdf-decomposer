{
  "name": "pdf-decomposer",
  "version": "1.2.0",
  "description": "A powerful PDF text and image extraction library with universal browser and Node.js support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "test": "npx tsx scripts/test.ts",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/febbyRG/pdf-decomposer.git"
  },
  "keywords": [
    "pdf",
    "parser",
    "typescript",
    "json",
    "browser",
    "angular",
    "react",
    "vue",
    "image-extraction"
  ],
  "author": "Febby Rachmat G<febby.rachmat@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/febbyRG/pdf-decomposer/issues"
  },
  "homepage": "https://github.com/febbyRG/pdf-decomposer#readme",
  "dependencies": {
    "change-case": "^5.4.4",
    "md5": "^2.3.0",
    "param-case": "^3.0.4",
    "pdfjs-dist": "^3.11.174",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
